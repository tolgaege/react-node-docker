{
  "name": "haystack-frontend",
  "dependencies": {
    "@material-ui/core": "^4.3.0",
    "@material-ui/icons": "^4.2.1",
    "@material-ui/styles": "^4.3.0",
    "apexcharts": "3.24.0",
    "axios": "0.19.0",
    "classnames": "^2.2.6",
    "font-awesome": "^4.7.0",
    "husky": "3.1.0",
    "lint-staged": "9.5.0",
    "lodash": "4.17.15",
    "mixpanel-browser": "2.32.0",
    "moment": "2.24.0",
    "mui-datatables": "^2.6.4",
    "prettier": "1.19.1",
    "react": "^16.8.6",
    "react-apexcharts": "1.3.3",
    "react-dom": "^16.8.6",
    "react-ga": "2.7.0",
    "react-github-login": "1.0.3",
    "react-google-maps": "^9.4.5",
    "react-hotjar": "2.0.4",
    "react-router-dom": "^5.0.1",
    "react-scripts": "^3.2.0",
    "react-syntax-highlighter": "^11.0.2",
    "react-toastify": "^5.3.2",
    "recharts": "1.8.5",
    "tinycolor2": "^1.4.1"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "src/**/*.{js,jsx,ts,tsx,json,css,scss,md}": [
      "prettier --write",
      "git add"
    ]
  },
  "eslintConfig": {
    "extends": [
      "react-app"
    ],
    "rules": {
      "no-unused-vars": "off"
    }
  },
  "proxy": "http://localhost:5000"
}
