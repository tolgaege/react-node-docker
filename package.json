{
  "name": "haystack",
  "scripts": {
    "start": "concurrently \"npm run watch-node\" \"cd frontend && npm start\"",
    "start:production": "npm run build:production && cd frontend && npm run build && cd .. && npm run server:production",
    "server": "node -r ts-node/register ./src/server.ts",
    "server:production": "node --max_old_space_size=2800 ./dist/server.js",
    "build": "tsc && npm run lint",
    "build:fast": "tsc",
    "build:production": "tsc --p tsconfig.production.json",
    "watch-node": "nodemon",
    "test": "jest --forceExit --coverage --verbose",
    "watch-test": "npm run test -- --watchAll",
    "lint": "tsc --noEmit && eslint \"**/*.{js,ts}\" --quiet --fix",
    "db:migration:run": "rm -rf ./dist && npm run build:fast && npm run typeorm -- migration:run",
    "db:migration:create": "rm -rf ./dist && npm run build:fast && npm run typeorm -- migration:generate",
    "db:migration:revert": "rm -rf ./dist && npm run build:fast && npm run typeorm -- migration:revert",
    "typeorm": "node ./node_modules/typeorm/cli.js"
  },
  "dependencies": {
    "@octokit/app": "4.1.0",
    "@octokit/webhooks": "6.3.2",
    "@types/cron": "^1.7.1",
    "async": "^3.1.0",
    "async-redis": "1.1.7",
    "bcrypt-nodejs": "^0.0.3",
    "bluebird": "^3.5.5",
    "body-parser": "^1.19.0",
    "compression": "^1.7.4",
    "cookie-parser": "1.4.4",
    "cors": "2.8.5",
    "cron": "^1.7.2",
    "d3": "5.12.0",
    "dotenv": "^8.1.0",
    "errorhandler": "^1.5.1",
    "express": "^4.17.3",
    "express-rate-limit": "5.0.0",
    "express-redis-cache": "1.1.3",
    "express-session": "^1.16.2",
    "express-validator": "^6.2.0",
    "fs": "0.0.1-security",
    "helmet": "3.21.2",
    "http-graceful-shutdown": "2.3.2",
    "jsonwebtoken": "8.5.1",
    "lodash": "^4.17.15",
    "lusca": "^1.6.1",
    "node-fetch": "2.6.0",
    "nodemailer": "^6.3.0",
    "octonode": "0.9.5",
    "parse-link-header": "1.0.1",
    "pg": "7.12.1",
    "redis": "2.8.0",
    "reflect-metadata": "0.1.13",
    "request-promise": "4.2.5",
    "typeorm": "0.2.20",
    "winston": "^3.2.1",
    "winston-daily-rotate-file": "4.2.1"
  },
